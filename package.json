{
  "name": "mailer",
  "version": "0.0.0",
  "private": true,
  "scripts": {
    "start": "node ./app.js",
    "dev": "nodemon ./app.js",
    "migrate:up": "db-migrate up --config src/database/database.json --migrations-dir src/database/migrations -v",
    "migrate:down": "db-migrate down -c 1 --config src/database/database.json -m src/database/migrations",
    "migrate:down:all": "db-migrate down -c 999 --config src/database/database.json -m src/database/migrations",
    "migrate:create": "db-migrate create --config src/database/database.json -m src/database/migrations"
  },
  "dependencies": {
    "bcrypt": "^5.1.1",
    "cookie-parser": "~1.4.4",
    "cors": "^2.8.5",
    "db-migrate": "^0.11.14",
    "db-migrate-pg": "^1.5.2",
    "debug": "~2.6.9",
    "dotenv": "^16.4.5",
    "express": "~4.16.1",
    "express-fileupload": "^1.5.0",
    "googleapis": "^140.0.1",
    "html-to-text": "^9.0.5",
    "http-errors": "~1.6.3",
    "jsonwebtoken": "^9.0.2",
    "morgan": "~1.9.1",
    "node-cron": "^4.2.1",
    "nodemailer": "^6.9.14",
    "nodemon": "^3.1.3",
    "pg": "^8.12.0",
    "pg-hstore": "^2.3.4",
    "prettier": "^3.3.1",
    "sequelize": "^6.37.3",
    "xlsx": "^0.18.5"
  },
  "type": "module",
  "prettier": {
    "useTabs": true,
    "printWidth": 120,
    "tabWidth": 4
  }
}
